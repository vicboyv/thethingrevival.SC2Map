    }
    else if ((EventDialogControl() == libNtve_gf_ValueFromDataTableDialogItem(true, "EvacSelection"))) {
        PlayerGroupLoopBegin(gv_playing);
        for ( ; !PlayerGroupLoopDone() ; PlayerGroupLoopStep() ) {
            IntLoopBegin(1, 7);
            for ( ; !IntLoopDone() ; IntLoopStep() ) {
                if ((IntLoopCurrent() == DialogControlGetSelectedItem(libNtve_gf_ValueFromDataTableDialogItem(true, "EvacSelection"), PlayerGroupLoopCurrent()))) {
                    lv_evac[IntLoopCurrent()] += 1;
